unalias -a
export PATH=`pwd`:/usr/local/rvm/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin
if [ -f /usr/local/rvm/bin/rvm ]; then
   source /usr/local/rvm/scripts/rvm
   rvm reload
fi
